<table class="table table-bordered table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody class="table-hover">    
      
    <% 
      employees.forEach(function(emp, index) {
    %>
        <tr scope="row">
          <td><%= index +1 %></td> 
         <td><%= emp.firstName %></td>
         <td><%= emp.lastName %></td>
         <td><%= emp.email  %></td>           
        </tr>      
    <%  
      });
    %>
  </tbody>
</table>
